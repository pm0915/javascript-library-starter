{
  "name": "rtcsdk",
  "version": "1.0.0",
  "description": "vrv rtc sdk",
  "main": "index.js",
  "exports": {
    ".": {
      "browser": {
        "require": "./dist/browser/rtcsdk.cjs",
        "default": "./index.js"
      },
      "default": {
        "require": "./dist/node/rtcsdk.cjs",
        "default": "./index.js"
      }
    }
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "cross-env NODE_ENV=production rollup -c -m",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "fix": "eslint --fix src/**/*.js",
    "prepare": "husky install",
    "release:dry": "release-it --dry-run --no-npm",
    "release:info": "release-it --release-version",
    "release:beta:no-npm": "release-it --preRelease=beta --no-npm",
    "release:beta": "release-it --preRelease=beta",
    "release:no-npm": "release-it --no-npm",
    "release:changelog:fix": "git add CHANGELOG.md",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pm0915/RTCSDK.git"
  },
  "keywords": [
    "webRTC",
    "mediasoup-client"
  ],
  "author": "Min Pan",
  "license": "MIT",
  "engines": {
    "node": ">=14.18.0"
  },
  "bugs": {
    "url": "https://github.com/pm0915/RTCSDK/issues"
  },
  "homepage": "https://github.com/pm0915/RTCSDK#readme",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "auto-changelog": "^2.4.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "release-it": "^15.10.3",
    "rollup": "^3.23.0"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}